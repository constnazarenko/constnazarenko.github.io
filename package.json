{
  "name": "cv-page",
  "version": "2.0.0",
  "description": "Personal CV presentation",
  "repository": {
    "type": "git",
    "url": "git@github.com:constnazarenko/constnazarenko.github.io.git"
  },
  "keywords": [
    "ts",
    "haiku"
  ],
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src/*",
    "dist/*"
  ],
  "author": {
    "name": "Kostiantyn Nazarenko",
    "email": "const@nazarenko.es"
  },
  "homepage": "https://constnazarenko.github.io/constnazarenko.github.io/",
  "private": true,
  "dependencies": {
    "classnames": "2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-redux": "9.2.0",
    "react-router-dom": "7.4.0",
    "redux": "5.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@svgr/webpack": "8.1.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "autoprefixer": "10.4.21",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "13.0.0",
    "css-loader": "7.1.2",
    "dotenv": "16.4.7",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-react": "7.37.4",
    "gh-pages": "6.3.0",
    "globals": "16.0.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "mini-css-extract-plugin": "2.9.2",
    "npm-check": "6.0.1",
    "postcss": "^8.5.3",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "10.1.5",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "sass-embedded": "1.85.1",
    "sass-loader": "16.0.5",
    "source-map-explorer": "2.5.3",
    "source-map-loader": "5.0.0",
    "style-loader": "4.0.0",
    "stylelint": "16.16.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-scss": "6.11.1",
    "tailwindcss": "^4.0.14",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "webpack": "5.98.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0",
    "webpack-merge": "6.0.1"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "check-types": "tsc",
    "compile": "tsc -p .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -b release --git git --cname nazarenko.es",
    "prepare": "husky",
    "pretest": "npm run compile",
    "start": "npm run start:dev",
    "start:dev": "webpack serve --config webpack.dev.js",
    "start-mocked": "ENABLE_API_MOCK=true npm start",
    "test": "",
    "analyze": "source-map-explorer 'dist/*.js' --html bundle-analysis.html",
    "update-interactive": "npm-check --update --save-exact",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint src --fix",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:css:fix": "stylelint 'src/**/*.scss' --fix",
    "prettier": "prettier --check src/ ./package.json ./README.md",
    "prettier:fix": "prettier --write --ignore-unknown src/ ./package.json ./README.md"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss,md}": "prettier --write",
    "*.{css,scss}": "stylelint --fix"
  },
  "engines": {
    "npm": ">=9",
    "node": ">=20"
  }
}
